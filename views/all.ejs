<div class="row">
<div class="full columns text-center">
	<h2> Viewing All Pages </h2>
</div>
</div>
<%dataset.forEach(function(data, index){%>
<div class="row">
	<div class="large-10 small-10 large-offset-1 small-offset-1 columns">
		<a href="/wiki/<%=data.title%>"><%=data.title%></a>
	</div>
</div>
<div class="row">
		<div class="large-5 small-5 large-offset-6 small-offset-6 columns">
			<h6> last updated <span id='<%="update"+index%>'> </span> </h6>
		</div>
	</div>
<div class="row">
	<div class="columns large-6 small-6 large-offset-3 small-offset-3">
		<hr>
	</div>
</div>
<%})%>


		<script>
		<%dataset.forEach(function(data, index){%>
			var offsetNo<%=index%> = moment.tz("<%=data.timestamp%>", offset).format("dddd, MMMM Do YYYY, h:mm a")
			$('<%="#update"+index%>').text(offsetNo<%=index%>)
		<%})%>
		</script>
